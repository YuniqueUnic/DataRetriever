<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <!-- 设置应用程序版本号 -->
    <Version>2.2.2.0</Version>
    <!-- 设置应用程序作者 -->
    <Authors>Yun</Authors>
    <!-- 设置应用程序产品名称 -->
    <Product>VSTSDataProvider</Product>
    <!-- 设置应用程序描述 -->
    <Description>Get the VSTS Query or Execute Information</Description>
    <!-- 设置程序集版本号 -->
    <AssemblyVersion>2.2.2.0</AssemblyVersion>
    <!-- 设置文件版本号-->
    <FileVersion>2.2.2.0</FileVersion>
    <!--设置软件图标-->
    <ApplicationIcon>DataProvider2.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Images\Add.png" />
    <None Remove="Images\AddPicture.png" />
    <None Remove="Images\AllDone.png" />
    <None Remove="Images\BackgroundHightlight.png" />
    <None Remove="Images\BoldText.png" />
    <None Remove="Images\CharacterGrowFont.png" />
    <None Remove="Images\CharacterShrinkFont.png" />
    <None Remove="Images\Download.png" />
    <None Remove="Images\EditCopy.png" />
    <None Remove="Images\EditCut.png" />
    <None Remove="Images\EditPaste.png" />
    <None Remove="Images\EditRedo.png" />
    <None Remove="Images\EditUndo.png" />
    <None Remove="Images\FontColor.png" />
    <None Remove="Images\ItalicText.png" />
    <None Remove="Images\ListBullets.png" />
    <None Remove="Images\ListNumbering.png" />
    <None Remove="Images\Menu.png" />
    <None Remove="Images\ParagraphCenterJustify.png" />
    <None Remove="Images\ParagraphDecreaseIndentation.png" />
    <None Remove="Images\ParagraphFullJustify.png" />
    <None Remove="Images\ParagraphIncreaseIndentation.png" />
    <None Remove="Images\ParagraphLeftJustify.png" />
    <None Remove="Images\ParagraphRightJustify.png" />
    <None Remove="Images\Translate.png" />
    <None Remove="Images\UnderlineText.png" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="DataProvider2.ico" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="Images\BackgroundHightlight.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" Version="1.1.39" />
    <PackageReference Include="MiniExcel" Version="1.30.3" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Language\Resource.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resource.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Language\Resource.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Resource.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\Add.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\AddPicture.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\AllDone.png">
