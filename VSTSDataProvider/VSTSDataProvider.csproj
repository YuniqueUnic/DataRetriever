    <Resource Include="Images\EditRedo.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\EditUndo.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
